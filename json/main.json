{
  "content": {
    "layout_file": "default",
    "contents": [
      {
        "id": "main-layout",
        "component": "layout",
        "props": {
          "as": "main",
          "display": "flex",
          "flex_direction": "col",
          "padding": 4,
          "margin": 2
        }
      },
      {
        "id": "inner-div",
