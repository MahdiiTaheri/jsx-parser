{
  "name": "backend",
  "version": "1.2.0",
  "type": "module",
  "bin": {
    "tsx-parser": "src/cli/index.ts"
  },
  "scripts": {
    "dev": "bun run --hot src/server.ts",
    "jsxToJson": "bun run src/cli/index.ts jsxToJson",
    "jsxToJson:all": "bun run src/cli/index.ts jsxToJson results/jsx-input -o results/json-output -l dashboard",
    "jsxToJson:watch": "bun run src/cli/index.ts jsxToJson results/jsx-input -o results/json-output -w",
    "jsonToJsx": "bun run src/cli/index.ts jsonToJsx",
    "jsonToJsx:all": "bun run src/cli/index.ts jsonToJsx results/json-output -o results/jsx-output",
    "test": "bunx jest --config test/jest.config.ts"
  },
  "dependencies": {
    "@babel/generator": "^7.26.9",
    "@babel/parser": "^7.26.9",
    "@babel/traverse": "^7.26.9",
    "@babel/types": "^7.26.9",
    "@hono/node-server": "^1.13.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/jest": "^29.5.14",
    "dotenv": "^16.4.7",
    "hono": "^4.7.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@types/bun": "latest",
    "@types/node": "^22.13.9",
    "chokidar": "^3.6.0",
    "commander": "^11.1.0"
  },
  "files": [
    "src"
  ]
}
