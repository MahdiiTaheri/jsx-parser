          "tagName": "div",
          "full-width": "true",
          "height": "full"
        },
        "parent_id": "main-layout"
      },
      {
        "id": "text",
        "component": "text",
        "props": {
          "is_underline": "true",
          "color": "danger",
          "size": "lg",
          "weight": "semibold",
          "as": "p",
          "children": "Paragraph text"
        },
        "parent_id": "inner-div"
      },
      {
        "id": "login-button",
        "component": "button",
        "props": {
          "as": "button"
        },
        "parent_id": "inner-div"
      },
      {
        "id": "button-text",
        "component": "text",
        "props": {
          "is_underline": "true",
          "color": "danger",
          "size": "lg",
          "weight": "semibold",
          "as": "p",
          "children": "Click me"
        },
        "parent_id": "login-button"
      },
      {
        "id": "icon",
        "component": "icon",
        "props": {
          "name": "home",
          "as": "icon"
        },
        "parent_id": "inner-div"
